uniform sampler2D Texture0 : TEXUNIT0;

void main( float2 in v_TexCoord : TEXCOORD0,
           float4 in v_Color    : COLOR0,
           float4 out Color     : COLOR)
{
	Color = tex2D(Texture0, v_TexCoord) * v_Color;
}
